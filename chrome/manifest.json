{
  "name": "Dominion Helper",
  "version": "0.1",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://dominion.games/"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Dominion Helper"
  }
}
